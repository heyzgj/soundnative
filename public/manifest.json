{
  "manifest_version": 3,
  "name": "SoundNative",
  "description": "Helps non-native speakers write like native English speakers.",
  "version": "1.0.2",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "192": "icons/icon192.png",
    "512": "icons/icon512.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.ico",
    "default_title": "SoundNative"
  },
  "permissions": [
    "storage", "scripting"
  ]
}
